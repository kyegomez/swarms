    ToolIntegration --> ToolRegistry
    ToolRegistry --> AgentCore
    
    CLI --> SwarmRouter
    Telemetry --> AgentCore
    Telemetry --> SwarmRouter
    
    AgentCore --> LLMAPIs
    ConvHandler --> VectorStores

